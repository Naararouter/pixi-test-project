{
  "name": "vite-vanilla-ts-lib-starter",
  "private": true,
  "version": "0.0.2",
  "main": "./dist/vite-ts-lib-starter.cjs",
  "module": "./dist/vite-ts-lib-starter.mjs",
  "exports": {
    ".": {
      "require": "./dist/vite-ts-lib-starter.cjs",
      "import": "./dist/vite-ts-lib-starter.mjs"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "serve": "./node_modules/.bin/http-server ./dist",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended": "^14.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "@pixi/filter-color-replace": "^5.1.1",
    "pixi.js": "^7.2.4"
  }
}
